<QinIM>
  <name>English</name>
  <tableim>false</tableim>
  <preeditable>false</preeditable>
  <customkeymap>true</customkeymap>
  <keymap>
    <normal>
      <key value="`">`</key>
      <key value="1">1</key>
      <key value="2">2</key>
      <key value="3">3</key>
      <key value="4">4</key>
      <key value="5">5</key>
      <key value="6">6</key>
      <key value="7">7</key>
      <key value="8">8</key>
      <key value="9">9</key>
      <key value="0">0</key>
      <key value="-">-</key>
      <key value="=">=</key>

      <key value="q">!</key>
      <key value="w">@</key>
      <key value="e">#</key>
      <key value="r">$</key>
      <key value="t">%</key>
      <key value="y">^</key>
      <key value="u">&amp;</key>
      <key value="i">*</key>
      <key value="o">(</key>
      <key value="p">)</key>
      <key value="[">[</key>
      <key value="]">]</key>
      <key value="\\">\</key>

      <key value="a">~</key>
      <key value="s">_</key>
      <key value="d">+</key>
      <key value="f">{</key>
      <key value="g">}</key>
      <key value="h">|</key>
      <key value="j">&lt;</key>
      <key value="k">&gt;</key>
